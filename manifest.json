{
  "manifest_version": 2,
  "icons": {
    "128": "icon-128.png"
  },
  "name": "Zimbra notifier",
  "short_name": "znotifier",
  "description": "This extension show notification whenever new message arrives in zimbra",
  "version": "1.1.2",
  "background": {
    "scripts": [
      "jquery-3.1.1.min.js",
      "soap.js",
      "utils.js",
      "zimbra-notifier.js",
      "background.js"
    ]
  },
  "permissions": [
    "background",
    "storage",
    "tabs",
    "notifications",
    "https://*.pentalog.fr/"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
